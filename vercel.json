{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "env": {
    "VITE_API_URL": "https://subscription-manager-3u8t.onrender.com/api/v1"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://subscription-manager-3u8t.onrender.com/api/v1"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://subscription-manager-3u8t.onrender.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}