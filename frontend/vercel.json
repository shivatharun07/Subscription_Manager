{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://subscription-manager-3u8t.onrender.com"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}